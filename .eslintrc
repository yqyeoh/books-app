{
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
      "ecmaVersion": 8,
      "ecmaFeatures": {
        "impliedStrict": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "no-console": "warn",
      "consistent-return": 0,
      "func-names": 0,
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "prettier/prettier": [
        "error"
      ]
    },
  
    "plugins": ["prettier", "import"]
  }